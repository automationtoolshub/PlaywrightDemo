{
  "name": "Playwright",
  "version": "1.0.0",
  "description": "PlaywrightDemo",
  "main": "index.js",
  "scripts": {
    "pretest": "rm -rf reports && rm -rf rm -rf allure* && rm -rf output",
    "test1": "npx cucumber-js test --format allure-cucumberjs/reporter --format-options '{\"resultsDir\": \"reports/allure-results\"}' --tags=@smoke || true",
    "test": "npx cucumber-js test --format allure-cucumberjs/reporter  --tags=@smoke || true",
    "cucumberReport": "npx ts-node src/helper/report/cucumberReport.ts",
    "allureReport": "npx allure generate reports/allure-results --clean -o reports/allure-report && npx allure open reports/allure-report",
    "posttest": "npx allure generate allure-results --clean -o allure-report && npx allure open allure-report && python3 -m http.server",
    "lint1": "node_modules/.bin/eslint . --ext .ts",
    "lint": "node_modules/.bin/eslint .",
    "clean": "npm cache clean --force"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@cucumber/cucumber": "^11.2.0",
    "@playwright/test": "1.49.1",
    "@types/node-wifi": "^2.0.2",
    "@types/ssh2": "^1.15.3",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "allure-commandline": "^2.32.0",
    "allure-cucumberjs": "^3.0.8",
    "allure-js-commons": "^3.0.8",
    "allure-playwright": "^3.0.8",
    "axios": "^1.7.9",
    "eslint": "^8.46.0",
    "fs-extra": "^11.2.0",
    "fs.promises": "^0.1.2",
    "http-server": "^14.1.1",
    "https": "^1.0.0",
    "moment": "^2.30.1",
    "multiple-cucumber-html-reporter": "^3.9.0",
    "prettier": "^3.4.2",
    "react-native-network-info": "^5.2.1",
    "react-native-wifi-reborn": "^4.13.1",
    "ssh2": "^1.16.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@types/node": "^22.10.5",
    "bcrypt": "^5.1.1",
    "crypto": "^1.0.1",
    "node-wifi": "^2.0.16",
    "node-wifi-cli": "^2.1.1",
    "node-wifi-scanner": "^1.1.3",
    "react-native-wifi": "^1.0.1",
    "systeminformation": "^5.25.10",
    "wifiscanner": "^1.0.1"
  }
}
